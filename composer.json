{
  "name": "gdbots/pbj",
  "homepage": "https://github.com/gdbots/pbj-php",
  "description": "Protobuf inspired message tools for php and json.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1",
    "ext-bcmath": "*",
    "beberlei/assert": "~2.4",
    "gdbots/common": "~1.1",
    "moontoast/math": "~1.1",
    "ramsey/uuid": "~3.5",
    "symfony/yaml": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.3",
    "aws/aws-sdk-php": "~3.18",
    "ruflin/elastica": "~3.2"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Pbj\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Pbj\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  }
}
