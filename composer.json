{
  "name": "gdbots/pbj",
  "homepage": "https://github.com/gdbots/pbj-php",
  "description": "Protobuf inspired message tools for php and json.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=5.4",
    "ext-bcmath": "*",
    "beberlei/assert": "~2.1",
    "gdbots/common": "~0.1.2",
    "symfony/yaml": "~2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.3",
    "aws/aws-sdk-php": "~2.8",
    "ruflin/elastica": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Pbj\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Pbj\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    }
  },
  "replace": {
    "gdbots/pbj-php": "self.version"
  }
}
