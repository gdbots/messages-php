{
  "name": "gdbots/pbj",
  "homepage": "https://github.com/gdbots/pbj-php",
  "description": "Protobuf inspired message tools for php and json.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.4",
    "ext-bcmath": "*",
    "ext-json": "*",
    "beberlei/assert": "^3.2",
    "brick/math": "^0.8",
    "ramsey/uuid": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1",
    "aws/aws-sdk-php": "^3.138",
    "ruflin/elastica": "^7.0.0-beta.4"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Pbj\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Pbj\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
